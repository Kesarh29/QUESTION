class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        int size=m+n;
        for(int i=0;(i<nums2.length)&&(m<size);i++)
        {
              nums1[m]=nums2[i];
              m=m+1;
        }
        Arrays.sort(nums1);
        System.out.println(Arrays.toString(nums1));
        
    }
}
